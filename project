//Detecting module

//Transducer Pulse Module
void setup() {
  pinMode(12, OUTPUT); // sets the digital pin 12 as output
 }
 
 void loop() {
  digitalWrite(12, HIGH); // sets the digital pin 12 on
  delay(1000); // waits for a second
  digitalWrite(12, LOW); // sets the digital pin 12 off
  delay(1000); // waits for a second
 }
 
 // Analog Processing Module
 int analogPin = A3; // potentiometer wiper (middle terminal) connected to analog pin 3
 
 int val = 0; // variable to store the value read
 
 void setup() {
  Serial.begin(9600); // setup serial
 }
 
 void loop() {
  val = analogRead(analogPin); // read the input pin
  Serial.printIn(val); // debug value
 }
 
 // Imaging Module (1)
 
 // Imaging Module (2)
 Serial.printIn()
